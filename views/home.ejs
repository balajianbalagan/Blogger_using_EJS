<%- include("partials/header") -%>
<div id="wrap">
  <div id="main" class="container clear-top">

    <h1>Home</h1>
    <div>
      <p>
        <%= homeStartingContent %>
        </p>
      </div>

      <h1>Posts</h1>
      <div class="container " style="margin-botton:50px;">
        <% for (var i = 0; i < posts.length; i++) { %>
          <div class="posts">
            <h3><%= posts[i].title %></h3>
            <hr class="hrp"/>
            <p><%= posts[i].body.substring(0,100) + "........ " %><a href="/posts/<%=posts[i].title%>">Read more</a></p>

            </div>

        <% } %>
        </div>
</div>
</div>
<%- include("partials/footer") -%>
